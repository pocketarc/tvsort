{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "google",
    "next/core-web-vitals",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "require-jsdoc": "off",
    "no-return-await": "error",
    "prefer-promise-reject-errors": "error",
    "no-console": "off",
    "no-invalid-this": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "valid-jsdoc": "off",
    "new-cap": "off"
  }
}